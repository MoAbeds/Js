{
  "expo": {
    "name": "Reporterr",
    "slug": "Reporterr",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "androidStatusBarColor": "#1E273B",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#1E273B"
    },
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",

    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#293551"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "svg"]
    }
  }
}
